#!/usr/bin/env python3
"""
scarfbench - Entry point script for ScarfBench CLI

This script allows running scarfbench directly: ./bin/scarfbench <command>
Or add bin/ to PATH: export PATH=$PATH:./bin && scarfbench <command>
"""

import sys
from pathlib import Path

script_dir = Path(__file__).parent
run_processing_dir = script_dir.parent
sys.path.insert(0, str(run_processing_dir))

from scarfbench.cli import main

if __name__ == '__main__':
    main()

